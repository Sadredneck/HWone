<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>mainServlet</servlet-name>
        <servlet-class>com.spring2k18.MainServlet</servlet-class>
        <init-param>
            <param-name>readOnly</param-name>
            <param-value>false</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainServlet</servlet-name>
        <url-pattern>/lemain</url-pattern>
    </servlet-mapping>
</web-app>